{
  "name": "ipfs-uniform-resolve",
  "version": "1.0.0",
  "description": "Resolve IPFS paths to data in a well-documented, uniform fashion",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "unpkg": "dist/index.umd.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "src",
    "dist",
    "LICENSE"
  ],
  "scripts": {
    "build": "microbundle --compress=false"
  },
  "keywords": [
    "multiformat",
    "multicodec",
    "ipld",
    "path",
    "resolve",
    "ipfs"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:misterupkeep/ipfs-uniform-resolve.git"
  },
  "author": "Upkeep",
  "license": "MIT",
  "devDependencies": {
    "microbundle": "^0.15.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@ipld/dag-cbor": "^7.0.2",
    "@ipld/dag-json": "^8.0.10",
    "@ipld/dag-pb": "^2.1.17",
    "ipfs-core-types": "^0.11.1",
    "multiformat-multicodec": "^1.0.0",
    "multiformats": "^9.7.1"
  }
}
